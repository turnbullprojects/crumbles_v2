%ul.nav.nav-tabs
  %li=link_to tag_icon(:list, pat(:list)), url(:entries, :index)
  %li.active=link_to tag_icon(:plus, pat(:new)), url(:entries, :new)

.tabs-content
  = form_for :entry, url(:entries, :create), :class => 'form-horizontal', :multipart => true do |f|
    =partial 'entries/form', :locals => { :f => f, :dictionaries => @dictionaries }
